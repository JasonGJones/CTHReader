<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="outputLocation" value="c:\temp\"/>
    <!--use the outputFilePrefix setting to prepend all of the output files with a common string e.g. "PROD" -->
    <add key="outputFilePrefix" value="PREPROD"/>
    
    <!--Enter the URL of the site collection (or content type hub) to be audited-->
    <add key="siteUrl" value="https://sp13/sites/QACTH/"/>
    
    <!--Enter "true" to be prompted for a user name and password or "false" to use your windows credentials-->
    <add key="passwordPrompt" value="false"/>
    
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
</configuration>